PVector[][] rectangle = {
	{new PVector(-10,-10), new PVector(10,-10)},
	{new PVector(-10,10), new PVector(10,10)},
	{new PVector(10,-10), new PVector(10,10)},
	{new PVector(-10,-10), new PVector(-10,10)}
};

PVector[][] triangle = {
	{new PVector(0,-10), new PVector(-10,10)},
	{new PVector(0,10), new PVector(10,10)},
	{new PVector(-10,10), new PVector(10,10)}
};
